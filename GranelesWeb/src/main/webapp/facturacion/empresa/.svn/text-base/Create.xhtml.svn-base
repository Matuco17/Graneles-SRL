<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.prime.com.tr/ui">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundleFacturacion.CreateEmpresaTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form>
                <h:panelGrid columns="4">
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_nombre}" for="nombre"  />
                    <p:inputText id="nombre" value="#{empresaController.selected.nombre}" />
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_direccion}" for="direccion"  />
                    <p:inputText id="direccion" value="#{empresaController.selected.direccion}" />
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_cuit}" for="cuit"  />
                    <p:inputText id="cuit" value="#{empresaController.selected.cuit}" />
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_cuidad}" for="cuidad"  />
                    <p:inputText id="cuidad" value="#{empresaController.selected.cuidad}" />
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_telefono}" for="telefono"  />
                    <p:inputText id="telefono" value="#{empresaController.selected.telefono}" />
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_mail}" for="mail"  />
                    <p:inputText id="mail" value="#{empresaController.selected.mail}" />
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_web}" for="web"  />
                    <p:inputText id="web" value="#{empresaController.selected.web}" />
                    <h:outputLabel value="#{bundleFacturacion.EmpresaTitle_tipoEmpresa}" for="tipoEmpresa"  />
                    <h:selectOneMenu id="tipoEmpresa" value="#{empresaController.selected.tipoEmpresa}" required="true" requiredMessage="#{bundleFacturacion.EmpresaRequiredMessage_tipoEmpresa}">
                        <f:selectItems value="#{fixedListController.tipoEmpresaSelectOne}"/>
                    </h:selectOneMenu>
                </h:panelGrid>
                <h:panelGrid columns="2">
                    <h:commandButton action="#{empresaController.create}"
                                     image="/faces/resources/images/save.png" 
                                     value="#{bundleFacturacion.EmpresaSaveLink}" 
                                     title="#{bundleFacturacion.EmpresaSaveLink}"
                                     />

                    <h:commandButton action="#{empresaController.prepareList}"
                                     image="/faces/resources/images/viewAll.png" 
                                     value="#{bundleFacturacion.EmpresaShowAllLink}"
                                     title="#{bundleFacturacion.EmpresaShowAllLink}"
                                     immediate="true" 
                                     />

                </h:panelGrid>
            </h:form>
        </ui:define>
    </ui:composition>

</html>
