<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.prime.com.tr/ui">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundlePersonal.ViewAccidentadoTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form>
                <h:panelGrid columns="4">
                    <h:outputText value="#{bundlePersonal.AccidentadoTitle_personal}" style="font-weight: bold;"/>
                    <h:outputText value="#{accidentadoController.selected.personal.toString()}" title="#{bundlePersonal.AccidentadoTitle_personal}"/>
                    <h:outputLabel value="" />
                    <h:outputLabel value="" />
                    <h:outputText value="#{bundlePersonal.AccidentadoTitle_tarea}" style="font-weight: bold;"/>
                    <h:outputText value="#{accidentadoController.selected.tarea.toString()}" title="#{bundlePersonal.AccidentadoTitle_tarea}"/>
                    <h:outputText value="#{bundlePersonal.AccidentadoTitle_categoria}" style="font-weight: bold;"/>
                    <h:outputText value="#{accidentadoController.selected.categoria.toString()}" title="#{bundlePersonal.AccidentadoTitle_categoria}"/>
                    <h:outputText value="#{bundlePersonal.AccidentadoTitle_desde}" style="font-weight: bold;"/>
                    <h:outputText value="#{accidentadoController.selected.desde}" title="#{bundlePersonal.AccidentadoTitle_desde}">
                        <f:convertDateTime pattern="dd/MM/yyyy" />
                    </h:outputText>
                    <h:outputText value="#{bundlePersonal.AccidentadoTitle_hasta}" style="font-weight: bold;"/>
                    <h:outputText value="#{accidentadoController.selected.hasta}" title="#{bundlePersonal.AccidentadoTitle_hasta}">
                        <f:convertDateTime pattern="dd/MM/yyyy" />
                    </h:outputText>
                    <h:outputText value="#{bundlePersonal.AccidentadoTitle_bruto}" style="font-weight: bold;"/>
                    <h:outputText value="#{accidentadoController.selected.bruto}" title="#{bundlePersonal.AccidentadoTitle_bruto}"/>
                </h:panelGrid>
                <h:outputText value="#{bundlePersonal.AccidentadoTitle_descripcionCortaAccidente}" style="font-weight: bold;"/>
                <h:outputText value="#{accidentadoController.selected.descripcionCortaAccidente}" title="#{bundlePersonal.AccidentadoTitle_descripcionCortaAccidente}"/>
           

                <h:panelGrid columns="4">
                    <h:commandButton action="#{accidentadoController.destroyAndView}" 
                                     image="/faces/resources/images/destroy.png" 
                                     value="#{bundlePersonal.AccidentadoDestroyLink}"
                                     title="#{bundlePersonal.AccidentadoDestroyLink}"
                                     onclick="return confirm('Â¿Esta seguro que desea eliminar este Accidentado?');" 
                                     />

                    <h:commandButton action="Edit" 
                                     image="/faces/resources/images/edit.png" 
                                     value="#{bundlePersonal.AccidentadoEditLink}"
                                     title="#{bundlePersonal.AccidentadoEditLink}"
                                     />
                    <h:commandButton action="#{accidentadoController.prepareCreate}" 
                                     image="/faces/resources/images/new.png" 
                                     value="#{bundlePersonal.AccidentadoCreateLink}"
                                     title="#{bundlePersonal.AccidentadoCreateLink}"
                                     />
                    <h:commandButton action="#{accidentadoController.prepareList}"
                                     image="/faces/resources/images/viewAll.png" 
                                     value="#{bundlePersonal.AccidentadoShowAllLink}"
                                     title="#{bundlePersonal.AccidentadoShowAllLink}"
                                     />
                </h:panelGrid>
            </h:form>
        </ui:define>
    </ui:composition>

