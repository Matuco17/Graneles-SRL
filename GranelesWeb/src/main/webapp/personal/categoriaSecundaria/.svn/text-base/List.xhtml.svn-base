<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.prime.com.tr/ui">

    <f:metadata>  
        <f:event type="preRenderView" listener="#{categoriaSecundariaController.init()}" />  
    </f:metadata> 


    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundlePersonal.ListCategoriaSecundariaTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form styleClass="jsfcrud_list_form">
                <h:outputText escape="false" value="#{bundlePersonal.CategoriaSecundariaEmpty}" rendered="#{categoriaSecundariaController.items.rowCount == 0}"/>
                <h:panelGroup rendered="#{categoriaSecundariaController.items.rowCount > 0}">
                    <p:dataTable value="#{categoriaSecundariaController.items}" 
                                 var="item"
                                 paginator="true" rows="10"
                                 paginatorTemplate="{CurrentPageReport}  {FirstPageLink} {PreviousPageLink} {PageLinks} {NextPageLink} {LastPageLink} {RowsPerPageDropdown}"
                                 rowsPerPageTemplate="5,10,15">
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundlePersonal.CategoriaSecundariaTitle_id}"/>
                            </f:facet>
                            <h:outputText value="#{item.id}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundlePersonal.CategoriaSecundariaTitle_registro}"/>
                            </f:facet>
                            <h:outputText value="#{item.registro}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundlePersonal.CategoriaSecundariaTitle_personal}"/>
                            </f:facet>
                            <h:outputText value="#{item.personal}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundlePersonal.CategoriaSecundariaTitle_categoria}"/>
                            </f:facet>
                            <h:outputText value="#{item.categoria}"/>
                        </p:column>
                        <p:column>
                            <f:facet name="header">
                                <h:outputText value="&nbsp;"/>
                            </f:facet>
                            <h:commandButton 
                                image="/faces/resources/images/view.png" 
                                action="#{categoriaSecundariaController.prepareView}" 
                                value="#{bundlePersonal.CategoriaSecundariaViewLink}"
                                title="#{bundlePersonal.CategoriaSecundariaViewLink}"
                                />

                            <h:commandButton 
                                image="/faces/resources/images/edit.png" 
                                action="#{categoriaSecundariaController.prepareEdit}" 
                                value="#{bundlePersonal.CategoriaSecundariaEditLink}"
                                title="#{bundlePersonal.CategoriaSecundariaEditLink}"
                                />

                            <h:commandButton 
                                image="/faces/resources/images/destroy.png"
                                onclick="return confirm('Â¿Esta seguro que desea eliminar este CategoriaSecundaria?');" 
                                action="#{categoriaSecundariaController.destroy}" 
                                title="#{bundlePersonal.CategoriaSecundariaDestroyLink}"
                                value="#{bundlePersonal.CategoriaSecundariaDestroyLink}"
                                />
                        </p:column>
                    </p:dataTable>
                </h:panelGroup>
                <h:commandButton 
                    image="/faces/resources/images/new.png" 
                    action="#{categoriaSecundariaController.prepareCreate}" 
                    value="#{bundlePersonal.CategoriaSecundariaCreateLink}"
                    title="#{bundlePersonal.CategoriaSecundariaCreateLink}"
                    />
            </h:form>
        </ui:define>
    </ui:composition>

</html>
