                    <h:outputText value="#{bundleSalario.SueldoTitle_totalDeductivo}" style="font-weight: bold;"/>
                    <h:outputText value="#{sueldoController.selected.totalDeductivo}" title="#{bundleSalario.SueldoTitle_totalDeductivo}"/>
                    <h:outputText value="#{bundleSalario.SueldoTitle_totalNoRemunerativo}" style="font-weight: bold;"/>
