<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.prime.com.tr/ui">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundleCarga.EditArchivoEmbarqueTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form>
                <h:panelGrid columns="4">
                    <h:outputLabel value="#{bundleCarga.ArchivoEmbarqueTitle_id}" for="id" style="font-weight: bold;" />
                    <p:inputText id="id" value="#{archivoEmbarqueController.selected.id}" title="#{bundleCarga.ArchivoEmbarqueTitle_id}" />
                    <h:outputLabel value="#{bundleCarga.ArchivoEmbarqueTitle_url}" for="url" style="font-weight: bold;" />
                    <p:inputText id="url" value="#{archivoEmbarqueController.selected.url}" title="#{bundleCarga.ArchivoEmbarqueTitle_url}" />
                    <h:outputLabel value="#{bundleCarga.ArchivoEmbarqueTitle_embarque}" for="embarque" style="font-weight: bold;" />
                    <p:selectOneMenu id="embarque" value="#{archivoEmbarqueController.selected.embarque}"  required="true" requiredMessage="#{bundleCarga.ArchivoEmbarqueRequiredMessage_embarque}" style="width:100%;">
                        <f:selectItems value="#{embarqueController.itemsAvailableSelectOne}"/>
                    </p:selectOneMenu>
                </h:panelGrid>
                <h:panelGrid columns="3">

                    <h:commandButton action="#{archivoEmbarqueController.update}" 
                                     image="/faces/resources/images/save.png" 
                                     value="#{bundleCarga.ArchivoEmbarqueSaveLink}"
                                     title="#{bundleCarga.ArchivoEmbarqueSaveLink}"
                                     />
                    <h:commandButton action="View" 
                                     value="#{bundleCarga.ArchivoEmbarqueViewLink}" 
                                     image="/faces//resources/images/view.png" 
                                     immediate="true"
                                     title="#{bundleCarga.ArchivoEmbarqueViewLink}"
                                     />
                    <h:commandButton action="#{archivoEmbarqueController.prepareList}"
                                     image="/faces/resources/images/viewAll.png" 
                                     value="#{bundleCarga.ArchivoEmbarqueShowAllLink}"
                                     immediate="true"
                                     title="#{bundleCarga.ArchivoEmbarqueShowAllLink}"
                                     />
                </h:panelGrid>                 
            </h:form>
        </ui:define>
    </ui:composition>

</html>
