<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:p="http://primefaces.prime.com.tr/ui">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundleCarga.EditPuertoTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form>
                <h:panelGrid columns="4">
                    <h:outputLabel value="#{bundleCarga.PuertoTitle_id}" for="id" style="font-weight: bold;" />
                    <p:inputText id="id" value="#{puertoController.selected.id}" title="#{bundleCarga.PuertoTitle_id}" />
                    <h:outputLabel value="#{bundleCarga.PuertoTitle_nombre}" for="nombre" style="font-weight: bold;" />
                    <p:inputText id="nombre" value="#{puertoController.selected.nombre}" title="#{bundleCarga.PuertoTitle_nombre}" />
                </h:panelGrid>
                <h:panelGrid columns="3">

                    <h:commandButton action="#{puertoController.update}" 
                                     image="/faces/resources/images/save.png" 
                                     value="#{bundleCarga.PuertoSaveLink}"
                                     title="#{bundleCarga.PuertoSaveLink}"
                                     />
                    <h:commandButton action="View" 
                                     value="#{bundleCarga.PuertoViewLink}" 
                                     image="/faces//resources/images/view.png" 
                                     immediate="true"
                                     title="#{bundleCarga.PuertoViewLink}"
                                     />
                    <h:commandButton action="#{puertoController.prepareList}"
                                     image="/faces/resources/images/viewAll.png" 
                                     value="#{bundleCarga.PuertoShowAllLink}"
                                     immediate="true"
                                     title="#{bundleCarga.PuertoShowAllLink}"
                                     />
                </h:panelGrid>                 
            </h:form>
        </ui:define>
    </ui:composition>

</html>
